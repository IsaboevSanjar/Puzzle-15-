<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uz.isytem.puzzle15">

    <application
        android:name="uz.isytem.puzzle15.core.app.App"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Lesson5.NoActionBarAndStatusBar">
        <activity
            android:name="uz.isytem.puzzle15.ui.InfoActivity"
            android:exported="false" />
        <activity
            android:name="uz.isytem.puzzle15.ui.SettingActivity"
            android:exported="false" />
        <activity
            android:name="uz.isytem.puzzle15.ui.ProgrammaticallyActivity"
            android:exported="false" />
        <activity
            android:name="uz.isytem.puzzle15.ui.ResultSaveActivity"
            android:exported="false" />
        <activity
            android:name="uz.isytem.puzzle15.ui.ResultActivity"
            android:exported="false" />
        <activity
            android:name="uz.isytem.puzzle15.ui.StartActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="uz.isytem.puzzle15.ui.MainActivity" />
    </application>

</manifest>